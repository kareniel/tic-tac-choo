{
  "name": "tic-tac-choo",
  "version": "1.0.0",
  "description": "smol tic-tac-toe game",
  "main": "index.js",
  "scripts": {
    "start": "budo index:bundle.js --dir public --live --pushstate -- -t sheetify",
    "build": "mkdir -p dist && cp public/* dist/ && browserify index.js -o dist/bundle.js -t sheetify",
    "test": "standard --fix"
  },
  "keywords": [
    "tic-tac-toe",
    "choo"
  ],
  "author": "kareniel",
  "license": "Apache-2.0",
  "dependencies": {
    "bytespin": "^2.0.6",
    "choo": "^6.13.0",
    "hypercore": "^6.18.1",
    "multifeed": "^1.3.4",
    "pull-stream": "^3.6.8",
    "random-access-memory": "^3.0.0",
    "stream-to-pull-stream": "^1.7.2"
  },
  "devDependencies": {
    "budo": "^11.3.2",
    "choo-devtools": "^2.5.1",
    "sheetify": "^7.3.3",
    "tachyons": "^4.11.1"
  }
}
